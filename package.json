{
  "name": "gmok-front-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "clean": "gmok clean",
    "clean:cache": "gmok clean --cache",
    "dev": "gmok dev",
    "build": "gmok build",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm run stub",
    "lint": "npm-run-all lint:*",
    "change": "gmok change",
    "bump": "gmok bump",
    "lint:eslint": "eslint . --cache --max-warnings 0 --fix",
    "lint:prettier": "prettier . --write ",
    "lint:style": "stylelint \"{packages,apps,docs}/**/*.{css,less,scss}\" --fix",
    "prepare": "is-ci || husky install",
    "reinstall": "rimraf pnpm-lock.yaml && pnpm run clean && npm run bootstrap",
    "stub": "turbo run stub",
    "test": "vitest",
    "create-vscode-file": "gmok create-code-workspace"
  },
  "devDependencies": {
    "@gmok/cli": "workspace:*",
    "@gmok/config-lint": "workspace:*",
    "@gmok/tsconfig": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.8.2",
    "cross-env": "^7.0.3",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.5.5",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vitest": "^0.23.4"
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": ">16.15.0"
  }
}
