{
  "name": "gmok-front-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "build": "pnpm --filter @internal/scrtips build",
    "clean": "rimraf '{apps,packages,configs}/**/{dist,node_modules,.turbo}' && rimraf node_modules",
    "clean:cache": "rimraf '{clea,packages,configs}/**/{dist,.turbo}'",
    "dev": "pnpm --filter @internal/scrtips dev",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm run stub",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --cache --max-warnings 0 --fix",
    "lint:prettier": "prettier . --write ",
    "lint:style": "stylelint \"{packages,apps,website}/**/*.{css,less,scss}\" --fix",
    "prepare": "is-ci || husky install",
    "reinstall": "rimraf pnpm-lock.yaml && pnpm run clean && npm run bootstrap",
    "stub": "turbo run stub",
    "test": "vitest",
    "create-vscode-file": "cc create-code-workspace"
  },
  "devDependencies": {
    "@config/lint": "workspace:*",
    "@scripts/cli-collection": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.3",
    "cross-env": "^7.0.3",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "ts-node": "^10.8.2",
    "tsconfig": "workspace:*",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vitest": "^0.17.1"
  },
  "packageManager": "pnpm@7.5.0",
  "engines": {
    "node": ">16.15.0"
  }
}
